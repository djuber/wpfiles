<p id="notice"><%= notice %></p>

<h1>Wordpresses</h1>

<table>
  <thead>
    <tr>
      <th>Version</th>
      <th>Release date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wordpresses.each do |wordpress| %>
      <tr>
        <td><%= wordpress.version %></td>
        <td><%= wordpress.release_date %></td>
        <td><%= link_to 'Show', wordpress %></td>
        <td><%= link_to 'Edit', edit_wordpress_path(wordpress) %></td>
        <td><%= link_to 'Destroy', wordpress, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wordpress', new_wordpress_path %>
